                    <ui:Instance template="TopToRight" style="scale: -1 1;" />
                    <ui:Instance template="Counterclockwise" style="scale: -1 1;" />
                    <ui:Instance template="WestToUp" style="scale: -1 1;" />
                    <ui:Instance template="TopToRight" style="scale: -1 1;" />
                    <ui:Instance template="WestToDown" style="scale: -1 1;" />
                    <ui:Instance template="TopToLeft" style="scale: -1 1;" />
