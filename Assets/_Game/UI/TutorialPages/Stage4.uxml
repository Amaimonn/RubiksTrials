            </ui:VisualElement>
            <ui:VisualElement name="transition-arrow" class="transition-arrow" />
            <ui:VisualElement name="state-container" class="state-container">
                <ColorReplacer fill-colors="#606060FF,#FF9D18FF,#606060FF,#FFF415FF,#FF9D18FF,#FFFFF9FF,#606060FF,#FF9D18FF,#606060FF,#606060FF,#3B50F9FF,#606060FF,#3B50F9FF,#3B50F9FF,#3B50F9FF,#3B50F9FF,#3B50F9FF,#3B50F9FF,#606060FF,#FF9D18FF,#606060FF,#FFFFF9FF,#FFFFF9FF,#FFFFF9FF,#FFFFF9FF,#FFFFF9FF,#FFFFF9FF" box-sizing="true" class="state__image" />
                <ui:Label tabindex="-1" text="Собран верхний крест (без учёта поворота граней)" display-tooltip-when-elided="true" name="state__label" class="state__label" />
            </ui:VisualElement>
        </ui:VisualElement>
        <ui:Label text="Грани (двухцветные срединные части ) необходимо перемещать, пока они не встанут на свои места, сделать это можно перестановкой соответствующих частей куба. Поменять две грани местами можно по следующей схеме:" name="schemes-description" class="content__paragraph" />
        <ui:VisualElement name="schemes-container" class="schemes-container">
            <ui:VisualElement name="scheme" class="scheme">
                <ColorReplacer fill-colors="#606060FF,#606060FF,#606060FF,#606060FF,#FF9D18FF,#FF9D18FF,#606060FF,#3B50F9FF,#606060FF,#606060FF,#FF9D18FF,#606060FF,#FFFFF9FF,#FFFFF9FF,#FFFFF9FF,#FFFFF9FF,#FFFFF9FF,#FFFFF9FF,#606060FF,#FFFFF9FF,#606060FF,#3B50F9FF,#3B50F9FF,#3B50F9FF,#3B50F9FF,#3B50F9FF,#3B50F9FF" box-sizing="true" class="state__image" style="scale: -1 1;" />
