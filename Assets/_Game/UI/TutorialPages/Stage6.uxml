                    <ui:VisualElement class="straight-arrow" style="flex-grow: 1; background-image: url(&quot;project://database/Assets/_Game/Images/Arrows/swipe_arrow.png?fileID=2800000&amp;guid=3840095536594574d848ecd31fe92c84&amp;type=3#swipe_arrow&quot;); -unity-background-scale-mode: scale-to-fit; top: 3px; position: absolute; width: 100px; height: 24px; left: 32px; scale: -1.4 0.5;" />
                    <ui:VisualElement class="straight-arrow" style="flex-grow: 1; background-image: url(&quot;project://database/Assets/_Game/Images/Arrows/swipe_arrow.png?fileID=2800000&amp;guid=3840095536594574d848ecd31fe92c84&amp;type=3#swipe_arrow&quot;); -unity-background-scale-mode: scale-to-fit; top: 39px; position: absolute; width: 53px; height: 32px; left: 102px; rotate: -212deg; scale: -1 1;" />
