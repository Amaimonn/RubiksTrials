                <ColorReplacer fill-colors="#606060FF,#606060FF,#55FF4CFF,#606060FF,#606060FF,#606060FF,#FFF415FF,#606060FF,#FF9D18FF,#55FF4CFF,#606060FF,#FFF415FF,#606060FF,#606060FF,#606060FF,#606060FF,#606060FF,#606060FF,#3B50F9FF,#606060FF,#FF9D18FF,#606060FF,#606060FF,#606060FF,#606060FF,#606060FF,#606060FF" box-sizing="true" class="state__image">
                    <ui:VisualElement class="straight-arrow" style="flex-grow: 1; background-image: url(&quot;project://database/Assets/_Game/Images/Arrows/swipe_arrow.png?fileID=2800000&amp;guid=3840095536594574d848ecd31fe92c84&amp;type=3#swipe_arrow&quot;); -unity-background-scale-mode: scale-to-fit; top: 3px; position: absolute; width: 100px; height: 24px; left: 32px; scale: 1.4 0.5;" />
